package piv_test

import (
	"testing"

	"github.com/stretchr/testify/require"

	pivx "cunicu.li/hawkes/ecdh/piv"
	"cunicu.li/hawkes/internal/test"
)

func TestPIV(t *testing.T) {
	require := require.New(t)

	skAlice, err := pivx.LoadPrivateKey()
	require.NoError(err)

	test.TestKeypair(t, skAlice)
}
